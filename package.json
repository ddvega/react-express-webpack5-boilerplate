{
  "name": "webpack-boilerplate",
  "version": "2.0.0",
  "description": "Webpack 5 boilerplate",
  "main": "index.js",
  "author": "David Vega",
  "license": "MIT",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node server/server",
    "dev": "concurrently \"npm run dev-client\" \"npm run dev-server\"",
    "dev-client": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.cjs",
    "dev-server": "cross-env NODE_ENV=development nodemon --experimental-specifier-resolution=node ./server/server",
    "build": "rimraf ./build && webpack --config config/webpack.prod.cjs",
    "lint": "eslint . --fix",
    "gcp-build": "npm run build"
  },
  "type": "module",
  "keywords": [
    "webpack",
    "webpack 5",
    "webpack boilerplate"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.2.2",
    "@google-cloud/firestore": "^4.9.8",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@netflix/nerror": "^1.1.3",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "clsx": "^1.1.1",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "firebase": "^8.3.2",
    "firebase-admin": "^9.6.0",
    "formik": "^2.2.6",
    "http-errors": "^1.8.0",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.3",
    "morgan": "^1.10.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.0.0",
    "copy-webpack-plugin": "^8.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.0",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "dotenv-webpack": "^7.0.2",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.3",
